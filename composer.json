{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "artesaos/seotools": "*",
        "cviebrock/eloquent-sluggable": "*",
        "guzzlehttp/guzzle": "^7.4",
        "jorenvanhocht/laravel-share": "^4.2",
        "lab404/laravel-impersonate": "^1.7",
        "laravel/framework": "^10.0",
        "laravel/jetstream": "^2.3",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.5",
        "livewire/livewire": "^2.6.2",
        "mews/purifier": "^3.4",
        "renatomarinho/laravel-page-speed": "dev-master",
        "spatie/laravel-analytics": "^5.0",
        "spatie/laravel-backup": "^8.3",
        "spatie/laravel-feed": "^4.3",
        "spatie/laravel-html": "^3.2",
        "spatie/laravel-newsletter": "^5.1",
        "spatie/laravel-sitemap": "^6.3",
        "spatie/schema-org": "^3.14",
        "yoeunes/toastr": "^2.3"
    },
    "require-dev": {
        "spatie/laravel-ignition": "^2.0",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files":[
            "app/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
